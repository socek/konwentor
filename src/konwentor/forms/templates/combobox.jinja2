<div class="form-group">
    <label style="display: block;">{{label}}:</label>
    {%- if error %} <div class="label label-danger">{{message}}</div>{%- endif -%}

    <input
            class="form-control"
            value="{{value}}"
            name="{{name}}"
            id="{{id}}"
            {%- if disabled %} disabled=disabled {% endif -%}
            >
    <ul id="{{id}}_list" style="display: none;">
        {%- for element in field.data -%}
            {%- if element['value'] != '' -%}
                <li>{{element['label']}}</li>
            {%- endif -%}
        {%- endfor -%}
    </ul>
</div>
