<div class="navbar-default sidebar" role="navigation">
    <div class="sidebar-nav navbar-collapse">
        <ul class="nav" id="side-menu">
            {% for object in menu %}
                <li>
                    <a {%- if object.is_highlited() %} class="active"{% endif %} href="{{object.get_url()}}">
                        <i class="fa {{object.get_icon()}} fa-fw"></i> {{object.name}}
                    </a>
                </li>
            {% endfor %}
        </ul>
    </div>
</div>
